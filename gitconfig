[user]
	name = Nate Fischer
	email = ntfschr@gmail.com
[core]
	editor = vim
[color]
	ui = true
[credential]
	helper = cache --timeout 3600
[alias]
	back = reset --hard HEAD~1
	co = checkout
	st = status
	squash = "!f() { git rebase -i HEAD~$1; }; f"
	br = branch
	brv = branch -v
	pc = !git-precommit.js
	precommit = !git-precommit.js
	rv = remote -v
	cob = checkout -b
	head = "!f() { [ -z $1 ] && git diff HEAD || git diff \"HEAD~$1\"; }; f"
[push]
	default = simple
